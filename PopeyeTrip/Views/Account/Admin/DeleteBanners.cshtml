@{
    ViewBag.Title = "DeleteBanners";
}

<h2>DeleteImages</h2>
@using (Html.BeginForm("DeleteBanners", "Admin", FormMethod.Post))
{
    <table style="background-color:azure" border="1">
        <thead style="background-color:gray">
            <tr>
                <th>Select</th>
                <th>Image</th>
            </tr>
        </thead>
        <tbody>
            @Html.EditorForModel()
